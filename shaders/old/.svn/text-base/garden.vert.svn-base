varying float zPos;
varying float yPos;

uniform float time;
uniform vec4 cPos;
uniform vec4 lightPos;

attribute float birth;
attribute float scale;

void main()
{
     
time=0.0;
cPos=vec4(0.0,0.0,0.0,0.0) ;
lightPos=vec4(0.0,0.0,0.0,0.0) ;
birth=0.0;
scale=0.0;

zPos=gl_Vertex.z;
yPos=gl_Vertex.y;
gl_Position = ftransform();
}
